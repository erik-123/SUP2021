             x:Class="SUP2021.Views.Test.FAQ"
              Title="{Binding Title}">

 
        
      <ContentPage.Content>
        
  


       <behaviors:Expander>
            <behaviors:Expander.Header>
                <Label Text="Om oss ▼"
